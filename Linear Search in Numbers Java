public class Main {
    public static void main(String[] args) {
        // Array of numbers
        int[] a = {10, 20, 30, 40, 50};

        // Number to search for
        int num = 30;
        
        // Variable to check if number is found
        int temp = 0;

        // Iterate through the array to find the number
        for (int i = 0; i < a.length; i++) {
            if (a[i] == num) {
                System.out.println("Number " + num + " is present at index " + i);
                temp++;
            }
        }

        // If number is not found
        if (temp == 0) {
            System.out.println("Number " + num + " not found in this list");
        }
    }
}
