class Main {
    public static void main(String[] args) {
        int a = 5;
        int b = 45;
        int c = 59;
        
        if (a > b && a > c)
            System.out.println("a is Greater");
        else if (b > c && b > a)
            System.out.println("b is Greater");
        else
            System.out.println("c is Greater");
    }
}
