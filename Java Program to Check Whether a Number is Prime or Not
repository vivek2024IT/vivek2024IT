public class Main {
    public static void main(String[] args) {
        int numberToCheck = 7;
        int temp = 0;
        
        for (int i = 2; i < numberToCheck - 1; i++) {
            temp = temp + 1;
        }

        if (temp == 0) {
            System.out.println(numberToCheck + " is an Even Number");
        } else {
            System.out.println(numberToCheck + " is an Odd Number");
        }
    }
}
